{
  "name": "@cerebrobot/chat-shared",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./src/*": "./src/*"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx,js,json}\"",
    "test": "vitest run"
  },
  "dependencies": {
    "zod": "4.1.11"
  },
  "devDependencies": {
    "vitest": "1.6.0"
  }
}
